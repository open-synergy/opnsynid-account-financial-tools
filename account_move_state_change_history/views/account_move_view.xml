<odoo>
    <record id="account_move_view_form" model="ir.ui.view">
        <field name="name">account.move.state.change.history.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[last()]" position="after">
                <page name="page_state_change_history" string="States Change History">
                    <field name="state_change_history_ids" nolabel="1" />
                </page>
            </xpath>
        </field>
    </record>
</odoo>
