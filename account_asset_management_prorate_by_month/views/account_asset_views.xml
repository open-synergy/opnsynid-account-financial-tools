<record id="account_asset_view_form" model="ir.ui.view">
    <field name="name">Prorate by Month</field>
    <field name="model">account.asset.asset</field>
    <field
                name="inherit_id"
                ref="account_asset_management.view_account_asset_asset_form"
            />
